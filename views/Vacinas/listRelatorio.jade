extends ../layout
block content
		.container
			.row
				table(id="tabela").bordered.striped.responsive-table.cols12
					thead
						th Vacina
						th Paciente
						th Data de Aplicação
						th Vacinador
						th Coren Vacinador
						tr
					tbody
						for val in vacinas
							for d in val.dados
								tr
								td #{val.nome}
								td #{d.paciente.nome}
								td #{moment(d.data).format('DD/MM/YYYY')}
								td #{d.aplicador.nome}
								td #{d.aplicador.coren}
extends ../layout

block content
    h1 Atualização de vacinas
    hr
    br
    form(method="POST", action="/vacina/edit/#{dados._id}", class="col s12")
      div(class="row")
        label Nome:
        input(type="text", name="nome", class="form-control", value="#{dados.nome}")
        button(type="submit", class="waves-effect waves-light btn") Atualizar
extends ../layout
block content
	.container
		h1 Pacientes
		p #{paciente.nome}
		br 
		p #{paciente._id}
		.container
			.row
				table
					thead
						th Vacina
						th Data Tomada
						tr
					tbody
						for val in paciente.vacinas
							tr
								td #{val.vacina.nome}
								td #{val.data}
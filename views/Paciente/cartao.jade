extends ../layout
block content
		.container
			h1 Cart√£o Paciente
			.row
				table.responsive-table
						thead
							th Nome: #{'' || paciente.nome}
							th CPF: #{'' || paciente.cpf}
							th E-Mail: #{'' || paciente.email}

				.row
					table.bordered.striped.responsive-table
						thead
							th Vacina
							th Data Tomada
							tr
						tbody
							for val in paciente.vacinas
								tr
									td #{val.vacina.nome}
									td #{moment(val.data).format('DD/MM/YYYY')}
extends ../layout
block content
		.container
			.row
				.row
					h3(id="pac").col.s5 Cartão Paciente
				div(id="pac").row
					h6.col.s4 NOME: #{'' || paciente.nome}
					h6.col.s3 CPF: #{'' || paciente.cpf}
					h6.col.s5 E-Mail: #{'' || paciente.email}

				.row
					table(id="tabela").bordered.striped.responsive-table.cols12
						thead
							th Vacina
							th Data de Aplicação
							th Aplicador
							th Coren Aplicador
							tr
						tbody
							for val in paciente.vacinas
								tr
									td #{val.vacina.nome}
									td #{moment(val.data).format('DD/MM/YYYY')}
									td #{val.aplicador.nome}
									td #{val.aplicador.coren}